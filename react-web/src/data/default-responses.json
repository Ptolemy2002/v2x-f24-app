{
    "other": [
        {
            "triggers": ["hello", "hi", "hey"],
            "type": "text",
            "text": "Hi there! How can I help you today?"
        },

        {   
            "triggers": ["how are you", "how are you doing"],
            "type": "text",
            "text": "I'm just code, but I'm here to assist you!"
        },

        {
            "triggers": ["what s your name", "what is your name", "who are you"],
            "type": "text",
            "text": "I don't have a name, but you can call me V2X Assistant!"
        },

        {
            "triggers": ["goodbye", "bye", "see you later"],
            "type": "text",
            "text": "Goodbye! Have a great day!"
        },

        {
            "triggers": ["send me a picture", "send me an image"],
            "type": "image",
            "src": "https://developers.elementor.com/docs/assets/img/elementor-placeholder-image.png",
            "alt": "Placeholder image"
        },

        {
            "triggers": ["send me a song", "send me some music"],
            "type": "audio",
            "src": "/aud-test.wav"
        }
    ],

    "[image]": {
        "type": "text",
        "text": "That's a nice image!"
    },

    "[audio]": {
        "type": "text",
        "text": "Great tunes!"
    },

    "[default]": {
        "type": "text",
        "text": "Sorry, I don't understand that."
    }
